{
  "manifest_version": 3,
  "name": "Week Number",
  "version": "1.9",
  "icons": {
    "16": "icons/icon48.png",
    "48": "icons/icon48.png",
    "128": "icons/icon48.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Week Number",
    "default_icon": {
      "16": "icons/icon48.png",
      "24": "icons/icon48.png",
      "32": "icons/icon48.png"
    }
  },
  "description": "This extension will show the current week number, day, and date.",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "author": "behera.manas98@gmail.com",
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  }
}
